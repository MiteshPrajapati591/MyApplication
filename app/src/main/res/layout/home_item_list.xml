        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_20"
        android:elevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutTrip">

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_200"
            android:scaleType="centerCrop"
            android:src="@drawable/dummy_image" />
    </io.github.florent37.shapeofview.shapes.RoundRectView>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewSloganTitle"
        style="@style/textViewBlack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:text="Slogan :"
        android:textSize="@dimen/sp_12"
        app:layout_constraintEnd_toStartOf="@+id/textViewSlogan"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/roundRectViewImage" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textViewSlogan"
        style="@style/textViewBlack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Smooth as Silk / I Fly THAI"
        android:textSize="@dimen/sp_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewSloganTitle"
        app:layout_constraintTop_toTopOf="@+id/textViewSloganTitle"
        android:layout_marginStart="@dimen/dp_5"
        />

    <LinearLayout
        android:id="@+id/linearLayoutCountry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:orientation="horizontal"
        android:weightSum="2"
        app:layout_constraintEnd_toEndOf="parent"
